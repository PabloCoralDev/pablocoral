<style>
    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    body {
        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
        line-height: 1.6;
        color: #2d3748;
    }

    .portfolio-container {
        width: 100%;
        background: #ffffff;
    }

    .container {
        max-width: 1200px;
        margin: 0 auto;
        padding: 0 2rem;
    }

    /* Hero Section */
    .hero-section {
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
        color: white;
        padding: 6rem 2rem 4rem;
        text-align: center;
    }

    .profile-image {
        width: 160px;
        height: 160px;
        border-radius: 50%;
        object-fit: cover;
        border: 5px solid white;
        box-shadow: 0 10px 40px rgba(0, 0, 0, 0.3);
        margin-bottom: 1.5rem;
    }

    .hero-name {
        font-size: 3rem;
        font-weight: 700;
        margin-bottom: 0.5rem;
    }

    .hero-tagline {
        font-size: 1.4rem;
        margin-bottom: 0.25rem;
        opacity: 0.95;
    }

    .hero-subtitle {
        font-size: 1.1rem;
        margin-bottom: 2rem;
        opacity: 0.9;
    }

    .hero-links {
        display: flex;
        gap: 1rem;
        justify-content: center;
        flex-wrap: wrap;
    }

    .hero-link {
        display: flex;
        align-items: center;
        gap: 0.5rem;
        padding: 0.75rem 1.5rem;
        background: rgba(255, 255, 255, 0.2);
        backdrop-filter: blur(10px);
        border: 2px solid rgba(255, 255, 255, 0.3);
        border-radius: 10px;
        text-decoration: none;
        color: white;
        font-weight: 500;
        transition: all 0.3s ease;
    }

    .hero-link svg {
        width: 20px;
        height: 20px;
    }

    .hero-link:hover {
        background: rgba(255, 255, 255, 0.3);
        transform: translateY(-2px);
    }

    /* Section Styles */
    .section {
        padding: 5rem 0;
    }

    .section-title {
        font-size: 2.5rem;
        font-weight: 700;
        text-align: center;
        margin-bottom: 3rem;
        color: #2d3748;
    }

    /* About Section */
    .about-section {
        background: #f7fafc;
    }

    .about-content {
        max-width: 800px;
        margin: 0 auto;
        font-size: 1.125rem;
        line-height: 1.8;
    }

    .about-content p {
        margin-bottom: 1.5rem;
    }

    /* Experience Section */
    .experience-grid {
        display: grid;
        gap: 2rem;
    }

    .experience-card {
        background: white;
        border-radius: 15px;
        padding: 2rem;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
        transition: transform 0.3s ease, box-shadow 0.3s ease;
    }

    .experience-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);
    }

    .card-header {
        display: flex;
        gap: 1.5rem;
        margin-bottom: 1.5rem;
        align-items: flex-start;
    }

    .company-logo {
        width: 60px;
        height: 60px;
        border-radius: 10px;
        object-fit: contain;
        background: #f7fafc;
        padding: 0.5rem;
    }

    .card-header h3 {
        font-size: 1.4rem;
        font-weight: 600;
        margin-bottom: 0.25rem;
        color: #2d3748;
    }

    .company {
        font-size: 1.05rem;
        color: #667eea;
        font-weight: 500;
    }

    .date {
        font-size: 0.95rem;
        color: #718096;
    }

    .experience-list {
        list-style: none;
        padding-left: 0;
    }

    .experience-list li {
        padding-left: 1.5rem;
        margin-bottom: 0.75rem;
        position: relative;
        color: #4a5568;
    }

    .experience-list li:before {
        content: "▹";
        position: absolute;
        left: 0;
        color: #667eea;
        font-weight: bold;
    }

    /* Projects Section */
    .projects-section {
        background: #f7fafc;
    }

    .projects-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
        gap: 2rem;
    }

    .project-card {
        background: white;
        border-radius: 15px;
        overflow: hidden;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
        transition: transform 0.3s ease, box-shadow 0.3s ease;
    }

    .project-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);
    }

    .project-image {
        width: 100%;
        height: 200px;
        object-fit: cover;
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    }

    .project-content {
        padding: 1.5rem;
    }

    .project-content h3 {
        font-size: 1.3rem;
        font-weight: 600;
        margin-bottom: 0.75rem;
        color: #2d3748;
    }

    .project-content p {
        color: #4a5568;
        margin-bottom: 1rem;
        line-height: 1.6;
    }

    .project-tags {
        display: flex;
        flex-wrap: wrap;
        gap: 0.5rem;
    }

    .tag {
        background: #edf2f7;
        color: #667eea;
        padding: 0.4rem 0.9rem;
        border-radius: 20px;
        font-size: 0.85rem;
        font-weight: 500;
    }

    /* Skills Section */
    .skills-categories {
        display: grid;
        gap: 2.5rem;
    }

    .skill-category {
        background: white;
        padding: 2rem;
        border-radius: 15px;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
    }

    .category-title {
        font-size: 1.3rem;
        font-weight: 600;
        margin-bottom: 1rem;
        color: #667eea;
    }

    .skills-tags {
        display: flex;
        flex-wrap: wrap;
        gap: 0.75rem;
    }

    .skill-tag {
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
        color: white;
        padding: 0.6rem 1.25rem;
        border-radius: 25px;
        font-size: 0.95rem;
        font-weight: 500;
        transition: transform 0.2s ease, box-shadow 0.2s ease;
    }

    .skill-tag:hover {
        transform: translateY(-2px);
        box-shadow: 0 5px 15px rgba(102, 126, 234, 0.4);
    }

    /* Activities Section */
    .activities-section {
        background: #f7fafc;
    }

    .activities-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
        gap: 2rem;
    }

    .activity-card {
        background: white;
        border-radius: 15px;
        overflow: hidden;
        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
        transition: transform 0.3s ease, box-shadow 0.3s ease;
    }

    .activity-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);
    }

    .activity-image {
        width: 100%;
        height: 180px;
        object-fit: cover;
        background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
    }

    .activity-card h3 {
        font-size: 1.2rem;
        font-weight: 600;
        padding: 1.5rem 1.5rem 0.5rem;
        color: #2d3748;
    }

    .activity-date {
        color: #667eea;
        font-weight: 500;
        padding: 0 1.5rem 0.5rem;
        font-size: 0.9rem;
    }

    .activity-card p:last-child {
        padding: 0 1.5rem 1.5rem;
        color: #4a5568;
        line-height: 1.6;
    }

    /* Footer */
    .footer {
        background: #2d3748;
        color: white;
        text-align: center;
        padding: 2rem;
    }

    /* Responsive Design */
    @@media (max-width: 768px) {
        .hero-name {
            font-size: 2rem;
        }

        .hero-tagline {
            font-size: 1.1rem;
        }

        .section {
            padding: 3rem 0;
        }

        .section-title {
            font-size: 2rem;
        }

        .projects-grid {
            grid-template-columns: 1fr;
        }

        .activities-grid {
            grid-template-columns: 1fr;
        }

        .hero-links {
            flex-direction: column;
        }

        .card-header {
            flex-direction: column;
        }
    }
</style>